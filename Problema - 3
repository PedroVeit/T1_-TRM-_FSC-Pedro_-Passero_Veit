main
    sub v0,v0            
    add v1,zr,0x0040     
    add v3,zr,0x0080

loop
    ldw v2,v1            

    sub v4,v4            
    tlt v4,v2            
    beq v4,zr,advance   

    sub v5,v5            
    add v5,v0
    tlt v5,v2            
    beq v5,zr,advance    

    sub v0,v0           
    add v0,v2

advance
    add v1,v1,2         
    bne v1,v3,loop       

end
    stw v0,zr,0x0090     
    hlt    
