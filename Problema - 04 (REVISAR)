main
    add v0,zr,0x0040     
    add v2,zr,0x004a 

input_n
    ldw v3,zr,0xf010
    stw v3,v0
    add v0,v0,2
    bne v0,v2,input_n
    add v0,zr,0x0040 
    add v1,zr,0x0090
    add v2,zr,0x004a     

loop
    ldw v3,v0            
    and v4,v3,1          
    bne v4,zr,return         
    add v3,v3,1         

return
    stw v3,v1           
    add v0,v0,2          
    add v1,v1,2          
    bne v0,v2,loop       

end
    hlt                  
